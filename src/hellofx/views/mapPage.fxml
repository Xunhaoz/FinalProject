<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
      prefHeight="720.0" prefWidth="1280.0" spacing="10.0" xmlns="http://javafx.com/javafx/16"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="hellofx.controller.Route">
    <AnchorPane>
        <ImageView fitHeight="720.0" fitWidth="1280.0" pickOnBounds="true" preserveRatio="true">
            <Image url="@../resource/map.png"/>
        </ImageView>
        <Button fx:id="level" mnemonicParsing="false" layoutX="60.0" layoutY="180.0" onAction="#mapToLevel"
                stylesheets="@../CSS/buttons.css">
            <graphic>
                <ImageView fitHeight="443.0" fitWidth="334.0">
                    <Image url="@../resource/e5.png"/>
                </ImageView>
            </graphic>
        </Button>
        <Button fx:id="market" mnemonicParsing="false" layoutX="450.0" layoutY="60.0" onAction="#mapToMarket"
                stylesheets="@../CSS/buttons.css">
            <graphic>
                <ImageView fitHeight="257.0" fitWidth="318.0">
                    <Image url="@../resource/market.png"/>
                </ImageView>
            </graphic>
        </Button>
        <Button fx:id="dorm" mnemonicParsing="false" layoutX="500.0" layoutY="430.0" onAction="#mapToDorm"
                stylesheets="@../CSS/buttons.css">
            <graphic>
                <ImageView fitHeight="245.0" fitWidth="266.0">
                    <Image url="@../resource/dorm.png"/>
                </ImageView>
            </graphic>
        </Button>
        <Button fx:id="trash" mnemonicParsing="false" layoutX="820.0" layoutY="80.0" onAction="#mapToTrash"
                stylesheets="@../CSS/buttons.css">
            <graphic>
                <ImageView fitHeight="218.0" fitWidth="237.0">
                    <Image url="@../resource/trash2.png"/>
                </ImageView>
            </graphic>
        </Button>
        <Button fx:id="ad" mnemonicParsing="false" layoutX="850.0" layoutY="340.0" onAction="#mapToAd"
                stylesheets="@../CSS/buttons.css">
            <graphic>
                <ImageView fitHeight="225.0" fitWidth="352.0">
                    <Image url="@../resource/ad.png"/>
                </ImageView>
            </graphic>
        </Button>
        <Button fx:id="backButton" mnemonicParsing="false" layoutX="30.0" layoutY="630.0" onAction="#mapToStart"
                stylesheets="@../CSS/buttons.css" />
    </AnchorPane>
</VBox>
