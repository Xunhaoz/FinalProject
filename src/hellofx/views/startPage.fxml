      xmlns:fx="http://javafx.com/fxml/1" fx:controller="hellofx.controller.Route">
    <AnchorPane>
        <ImageView fitHeight="720.0" fitWidth="1280.0" pickOnBounds="true" preserveRatio="true">
            <Image url="@../resource/background_start.png" />
        </ImageView>
        <VBox alignment="CENTER" spacing="15.0">
            <padding>
                <Insets bottom="20.0" left="305.0" right="20.0" top="50.0"/>
            </padding>
            <ImageView fitHeight="360.0" fitWidth="720.0" pickOnBounds="true" preserveRatio="true">
                <Image url="@../resource/csie_war_logo.png" />
            </ImageView>
            <Button layoutX="544.0" layoutY="415.0" mnemonicParsing="false" style="-fx-background-color:#00000000"
                    onAction="#startToMap">
                <graphic>
                    <ImageView fitWidth="315" fitHeight="70">
                        <Image url="@../resource/start_button.png"/>
                    </ImageView>
                </graphic>
            </Button>
            <Button layoutX="420.0" layoutY="380.0" mnemonicParsing="false" style="-fx-background-color:#00000000"
