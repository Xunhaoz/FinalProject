                <Font size="36.0"/>
            </font>
        </Button>
        <Button fx:id="backButton" layoutX="30.0" layoutY="630.0" mnemonicParsing="false" onAction="#levelToMap"
                stylesheets="@../../styles/buttons.css"/>
